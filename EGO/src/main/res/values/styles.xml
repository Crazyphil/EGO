        <item name="turnRampLeft">@drawable/da_turn_ramp_left</item>
        <item name="turnRampRight">@drawable/da_turn_ramp_right</item>
        <item name="turnLeft">@drawable/da_turn_left</item>
        <item name="turnRight">@drawable/da_turn_right</item>
        <item name="turnRoundabout1">@drawable/da_turn_roundabout_1</item>
        <item name="turnRoundabout2">@drawable/da_turn_roundabout_2</item>
        <item name="turnRoundabout3">@drawable/da_turn_roundabout_3</item>
        <item name="turnRoundabout4">@drawable/da_turn_roundabout_4</item>
        <item name="turnRoundabout5">@drawable/da_turn_roundabout_5</item>
        <item name="turnRoundabout6">@drawable/da_turn_roundabout_6</item>
        <item name="turnRoundabout7">@drawable/da_turn_roundabout_7</item>
        <item name="turnRoundaboutExit">@drawable/da_turn_roundabout_exit</item>
        <item name="turnSharpLeft">@drawable/da_turn_sharp_left</item>
        <item name="turnSharpRight">@drawable/da_turn_sharp_right</item>
        <item name="turnSlightLeft">@drawable/da_turn_slight_left</item>
        <item name="turnSlightRight">@drawable/da_turn_slight_right</item>
        <item name="turnStraight">@drawable/da_turn_straight</item>
        <item name="turnUnknown">@drawable/da_turn_unknown</item>
        <item name="turnUTurn">@drawable/da_turn_uturn</item>
        <item name="turnUTurnRight">@drawable/da_turn_uturn_right</item>
    </style>

</resources>
